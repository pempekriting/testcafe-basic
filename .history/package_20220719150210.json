{
  "name": "testcafe-example",
  "version": "1.0.0",
  "description": "TestCafe Automation Example",
  "main": "index.js",
  "scripts": {
    "test": "testcafe chrome tests/firstTestWithSelectorTest.js",
    "test-meta-case": "testcafe chrome tests/firstTestWithMetaDataTest.js --test-meta env=production",
    "test-meta-fixture": "testcafe chrome tests/firstTestWithMetaDataTest.js --fixture-meta env=staging",
    "test-by-name": "testcafe chrome tests/firstTestWithMetaDataTest.js -t 'First Test'",
    "test-run-all-browsers": "testcafe all test/firstTestWithSelectortTest.js --skip-js-errors",
    "test-run-parallel": "testcafe -c 2 chrome tests/firstTestWithMetaDataTest.js --skip-js-errors",
    "test-live-mode": "testcafe chrome tests/firstTestWithMetaDataTest.js -L",
    "test-debug-fail": "testcafe chrome test/firstTestWithMetaDataTest.js --debug-on-fail",
    "test-screenshot-fail": "testcafe chrome test/firstTestWithMetaDataTest.js -s takeOnFails=true"
    
  },
  "keywords": [
    "testcafe",
    "automation"
  ],
  "author": "Kgs. Azzam Nizar",
  "license": "ISC",
  "dependencies": {
    "testcafe": "^1.20.0"
  }
}
